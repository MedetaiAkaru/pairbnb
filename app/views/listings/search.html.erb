<p id="notice"><%= notice %></p>

<h1>Searched Listing Listings</h1>

<table>
  <thead>
    <tr>
      <th>Location</th>
      <th>Address</th>
      <th>Number of guests</th>
      <th>Number of bedrooms</th>
      <th>Number of bathrooms</th>
      <th>Pets</th>
      <th>Smoker</th>
      <th>Price</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @listings.each do |listing| %>
      <tr>
        <td><%= listing.location %></td>
        <td><%= listing.address %></td>
        <td><%= listing.number_of_guests %></td>
        <td><%= listing.number_of_bedrooms %></td>
        <td><%= listing.number_of_bathrooms %></td>
        <td><%= listing.pets %></td>
        <td><%= listing.smoker %></td>
        <td><%= listing.price %></td>
        <td><%= listing.user_id %></td>
        <td><%= link_to 'Show', listing %></td>
        <td><%= link_to 'Edit', edit_listing_path(listing) %></td>
        <td><%= link_to 'Destroy', listing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Listing', new_listing_path %>
